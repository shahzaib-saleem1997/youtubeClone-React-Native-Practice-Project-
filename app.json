{
  "expo": {
    "name": "youtubeClone",
    "slug": "WashupRider",
    "platforms": ["ios", "android", "web"],
    "android": {
      "package": "com.shahzaibDev.ytclone",
      "versionCode": 1
    },
    "ios": {
      "bundleIdentifier": "com.shahzaibDev.ytclone",
      "buildNumber": "1.0.0"
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splashed.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },

    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
  
  }
}